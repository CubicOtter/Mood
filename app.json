{
  "expo": {
    "name": "Mood",
    "slug": "Mood",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_resized.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#9DB8FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.tanguythan.mood",
      "versionCode": 2,
      "icon": "./assets/images/logo_notification.png"
    },
    "notification": {
      "icon": "./assets/images/logo_notification.png",     
      "color": "#9DB8FF",
      "androidMode": "default"
    }
  }
}
